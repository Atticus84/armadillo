                "type": "object"
            },
            "HistoricPairData": {
                "properties": {
                    "marketCap": {
                        "$ref": "#/components/schemas/Statistic"
                    },
                    "price": {
                        "$ref": "#/components/schemas/Statistic"
                    }
                },
                "required": [
                    "price",
                    "marketCap"
