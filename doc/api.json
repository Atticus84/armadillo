                "required": [
                    "saBaseToken",
                    "saQuoteToken",
                    "saExFeePerTokenNum",
                    "saExFeePerTokenDen",
                    "saRewardPkh",
                    "saBaseAmount",
                    "saMinQuoteAmount"
